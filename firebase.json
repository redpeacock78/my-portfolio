{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
        "source": "**/*",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=604800"
        }]
    }],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
